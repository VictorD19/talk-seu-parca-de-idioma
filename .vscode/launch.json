{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug API",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/back-end/src/server.ts",
      "runtimeExecutable": "${workspaceFolder}/back-end/node_modules/.bin/tsx",
      "outFiles": ["${workspaceFolder}/back-end/dist/**/*.js"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/back-end/.env",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}